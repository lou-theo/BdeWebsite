{% extends 'base.html.twig' %}

{% block title %} {{ parent() }}{% endblock %}

{% block body %}
    <div class="mini">
    <h3 style="margin-bottom: 50px; width: 40%;margin-left: 30%; margin-top: 20px; text-align: center" class="alert-secondary alert">Liste des notifications :</h3>
    {% for notification in notifications %}
        <div class="alert alert-success" style="width: 80%;margin-left: 10%">
        {{ notification.message }}

        </div>
    {% endfor %}
    </div>
{% endblock %}